generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "rhel-openssl-1.0.x"]
  previewFeatures = ["views"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SubscribedTeleUser {
  chatId    BigInt    @id
  username  String?
  name      String?
  createdAt DateTime  @default(now())
  deletedAt DateTime?

  @@map("subscribed_tele_users")
}
